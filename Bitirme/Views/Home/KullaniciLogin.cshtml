
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>Kullanıcı Giriş Sayfası</title>
    <meta name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no">
    <link rel="stylesheet" href="~/Belgeler/Back/dist/css/site.min.css">
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,800,700,400italic,600italic,700italic,800italic,300italic" rel="stylesheet" type="text/css">

    <script src="~/Belgeler/Back/dist/js/site.min.js"></script>
    <style>

        body {
            padding-top: 40px;
            padding-bottom: 40px;
            background-color: #303641;
            color: #C1C3C6
        }
    </style>
</head>
<body>
    <div class="container">
        <div class="form-signin">
            <h3 class="form-signin-heading">Kullanıcı Girişi</h3>
            <div class="form-group">
                <div class="input-group">
                    <div class="input-group-addon">
                        <i class="glyphicon glyphicon-user"></i>
                    </div>
                    <input type="text" class="form-control" name="KullaniciAdi" id="YoneticiKullaniciAdi" placeholder="Kullanıcı Adı" autocomplete="off" />
                </div>
            </div>
            <div class="form-group">
                <div class="input-group">
                    <div class="input-group-addon">
                        <i class=" glyphicon glyphicon-lock "></i>
                    </div>
                    <input type="password" class="form-control" name="Sifre" id="YoneticiSifre" placeholder="Şifre" autocomplete="off" />
                </div>
            </div>
            <button class="btn btn-lg btn-primary btn-block" type="button" id="btnKullaniciGirisYap">Giriş Yap</button>
        </div>
    </div>
    <div class="clearfix"></div>
    <br><br>

</body>
</html>
<script src="~/Belgeler/Back/build/KullaniciBack/KullaniciAdminLogin.js"></script>
